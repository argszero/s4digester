<configuration>
    <appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss.sss}][%X{workId}][%p]:%m%n</pattern>
        </encoder>
    </appender>
    <logger name="org.s4digester.tourist" level="trace"/>
    <root level="debug">
        <appender-ref ref="Console"/>
    </root>
</configuration>